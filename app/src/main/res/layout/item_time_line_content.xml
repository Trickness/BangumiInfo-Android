<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_marginStart="8dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/time_line_base_text_view"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Ƹ̵̡Ӝ̵̨̄Ʒ 玩过 テニスの王子様 ドキドキサバイバル 海辺のSecret、テニスの王子様 ドキドキサバイバル 山麓のMystic、テニスの王子様 ぎゅっと! ドキドキサバイバル 海と山のLove Passion 3部游戏 "/>
        <TextView
            android:id="@+id/time_line_date_view"
            android:textAlignment="textEnd"
            android:layout_width="70dp"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/time_line_collection_info_container"
        android:orientation="vertical"
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/time_line_image_set_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>